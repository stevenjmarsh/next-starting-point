{
  "name": "next-starting-point",
  "version": "1.0.0",
  "description": "A project template using next.js, react, material-ui, jest, eslint, storybook, prettier.",
  "main": "index.js",
  "repository": "",
  "author": "stevenjmarsh <stevenjmarsh@users.noreply.github.com>",
  "license": "NONE / COMMERCIAL",
  "scripts": {
    "pretest": "yarn lint",
    "test": "yarn unit-test",
    "lint": "eslint . --ext .js,.jsx",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "export": "yarn build && next export",
    "storybook": "start-storybook -s ./public -p 9001",
    "unit-test": "NODE_ENV=test jest -c .jest-config.json --verbose false",
    "unit-test-w": "NODE_ENV=test jest -c .jest-config.json --verbose true --watch"
  },
  "dependencies": {
    "@material-ui/core": "^4.10.2",
    "@material-ui/icons": "^4.9.1",
    "clsx": "^1.1.0",
    "dotenv": "^8.2.0",
    "next": "^9.4.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-toast-notifications": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@storybook/addon-actions": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/addon-viewport": "^5.3.18",
    "@storybook/addons": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@testing-library/jest-dom": "^5.3.0",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
